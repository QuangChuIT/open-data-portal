<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 7.3.0//EN"
        "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_3_0.dtd">

<resource-action-mapping>
    <model-resource>
        <model-name>vn.vnpt.data.cms.admin</model-name>
        <portlet-ref>
            <portlet-name>vn_vnpt_data_cms_web_internal_portlet_DataCmsAdminPortlet</portlet-name>
        </portlet-ref>
        <root>true</root>
        <permissions>
            <supports>
                <action-key>ADD_CATALOG</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>VIEW_DATA_DATASET</action-key>
                <action-key>VIEW_DICT_DATATYPE</action-key>
            </supports>
            <site-member-defaults>
                <action-key>ADD_CATALOG</action-key>
            </site-member-defaults>
            <guest-defaults/>
            <guest-unsupported>
                <action-key>ADD_CATALOG</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>VIEW_DATA_DATASET</action-key>
                <action-key>VIEW_DICT_DATATYPE</action-key>
            </guest-unsupported>
        </permissions>
    </model-resource>
   <model-resource>
        <model-name>vn.vnpt.data.cms.model.Catalog</model-name>
        <portlet-ref>
            <portlet-name>vn_vnpt_data_cms_web_internal_portlet_DataCmsAdminPortlet</portlet-name>
        </portlet-ref>
        <root>true</root>
        <permissions>
            <supports>
                <action-key>ADD_ENTRY</action-key>
                <action-key>DELETE</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>UPDATE</action-key>
                <action-key>IMPORT_DATA_CATALOG</action-key>
                <action-key>VIEW</action-key>
            </supports>
            <site-member-defaults>
                <action-key>ADD_ENTRY</action-key>
                <action-key>VIEW</action-key>
            </site-member-defaults>
            <guest-defaults>
                <action-key>VIEW</action-key>
            </guest-defaults>
            <guest-unsupported>
                <action-key>ADD_ENTRY</action-key>
                <action-key>DELETE</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>IMPORT_DATA_CATALOG</action-key>
                <action-key>UPDATE</action-key>
            </guest-unsupported>
        </permissions>
    </model-resource>
</resource-action-mapping>